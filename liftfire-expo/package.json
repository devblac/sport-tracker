{
  "name": "liftfire-expo",
  "version": "1.0.1",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export:web",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:production": "eas build --platform android --profile production",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:ios:production": "eas build --platform ios --profile production",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "analyze": "npx expo export --platform web && npx source-map-explorer 'dist/**/*.js'",
    "prebuild": "npm run type-check && npm run lint",
    "validate": "npm run type-check && npm run lint && npm test"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@supabase/supabase-js": "^2.78.0",
    "expo": "~54.0.23",
    "expo-av": "~16.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-secure-store": "^15.0.7",
    "expo-sqlite": "~16.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.12",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "~29.5.14",
    "@types/react": "~19.1.0",
    "expo-asset": "^12.0.9",
    "jest": "~29.7.0",
    "jest-expo": "^54.0.13",
    "typescript": "~5.9.2"
  },
  "private": true
}
