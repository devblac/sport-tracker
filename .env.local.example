# Environment Configuration Template
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to version control

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Development Settings
VITE_ENVIRONMENT=development
VITE_APP_VERSION=dev

# Feature Flags (set to true/false as needed)
VITE_FEATURE_SOCIAL_FEATURES=true
VITE_FEATURE_PREMIUM_FEATURES=false
VITE_FEATURE_AI_RECOMMENDATIONS=false
VITE_FEATURE_NEW_WORKOUT_PLAYER=true
VITE_FEATURE_REAL_TIME_NOTIFICATIONS=false
VITE_FEATURE_ADVANCED_ANALYTICS=false
VITE_FEATURE_BETA_FEATURES=true

# Optional: Analytics and Monitoring (leave empty for development)
VITE_ERROR_TRACKING_DSN=
VITE_ANALYTICS_ID=